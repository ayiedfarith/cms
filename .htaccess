

RewriteEngine on

RewriteRule ^(index|contact|registration|search|login|reset)$ $1.php [NC,L]

RewriteRule  ^post/(\d+)$ post.php?p_id=$1 [NC,L]

RewriteRule  ^category/(\d+)$ category.php?category=$1 [NC,L]

RewriteRule ^(admin/index|admin/posts|admin/categories|admin/comments|admin/users|admin/profile|admin/dashboard)$ $1.php [NC,L]


